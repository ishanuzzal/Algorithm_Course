#include<bits/stdc++.h>

using namespace std;

int main(){
   int arr[5] = {0,7,5,5,1};
   int tk = 10;
   int amount[5][10];
   amount[0][0] = 1;
    amount[5][10] = {0};

    for(int i=1; i<5; i++){
        for(int j=0; j<tk; j++){
            if(amount[i-1][j] && j+arr[i]<=10){
                amount[i][j+arr[i]] = 
            }
        }
    }


}
